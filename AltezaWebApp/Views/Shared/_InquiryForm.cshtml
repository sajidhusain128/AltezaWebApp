
@{
    ViewData["Title"] = "_InquiryForm";
    Layout = null;
}

<section class="elementor-element elementor-element-824fa95 elementor-section-boxed elementor-section-height-default elementor-section-height-default elementor-section elementor-top-section" data-id=824fa95 data-element_type=section>
    <div class="elementor-container elementor-column-gap-default">
        <div class=elementor-row>
            <div class="elementor-element elementor-element-effef83 elementor-column elementor-col-100 elementor-top-column" data-id=effef83 data-element_type=column>
                <div class="elementor-column-wrap  elementor-element-populated">
                    <div class=elementor-widget-wrap>
                        <div class="elementor-element elementor-element-8268bdd eael-caldera-form-align-default eael-caldera-form-btn-align-default elementor-widget elementor-widget-eael-caldera-form" data-id=8268bdd data-element_type=widget data-widget_type=eael-caldera-form.default>
                            <div class=elementor-widget-container>
                                <div class=eael-caldera-form-container>
                                    <div class=caldera-grid id=caldera_form_1 data-cf-ver=1.8.9 data-cf-form-id=CF5dfc8db22e390>
                                        <div id=caldera_notices_1 data-spinner=https://www.infozium.com/wp-admin/images/spinner.gif></div>


                                        <div class="contact100-form validate-form">
                                            <!-- <span class="contact100-form-title">
                                                Contact Us
                                            </span> -->

                                            <div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Name is required">
                                                <!--<span class="label-input100">Your Name</span>-->
                                                <input class="input100" type="text" name="name" placeholder="Enter your name">
                                                <label class="label-input100-new" for="name">
                                                    <span class="fas fa-user"></span>
                                                </label>
                                                <span class="focus-input100"></span>
                                            </div>

                                            <div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                                                <!--<span class="label-input100">Email</span>-->
                                                <input class="input100" type="text" name="email" placeholder="Enter your email addess">
                                                <label class="label-input100-new" for="name">
                                                    <span class="fas fa-envelope"></span>
                                                </label>
                                                <span class="focus-input100"></span>
                                            </div>

                                            <div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Contact no is required">
                                                <!--<span class="label-input100">Contact No</span>-->
                                                <input class="input100" type="text" name="contact" placeholder="Enter your contact number" maxlength="15" onkeypress="return contactNoValidation(this,event)">
                                                <label class="label-input100-new" for="name">
                                                    <span class="fas fa-phone-alt"></span>
                                                </label>
                                                <span class="focus-input100"></span>
                                            </div>
                                            <div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Service type is required">
                                                <!--<span class="label-input100">Service of Interest</span>-->
                                                <select name="services" class=input100>
                                                    <option value="" selected>Select your service</option>
                                                    <option value="Email Campaign Management & Production">Email Campaign Management & Production</option>
                                                    <option value="Search Marketing (SEO/PPC) & Analytics">Search Marketing (SEO/PPC) & Analytics</option>
                                                    <option value="Content & Social Media Marketing">Content & Social Media Marketing</option>
                                                    <option value="Websites Development">Websites Development</option>
                                                    <option value="Mobile & Web Apps">Mobile & Web Apps</option>
                                                    <option value="Software Development"> Software Development</option>
                                                    <option value="Rewards & Recognitions">Rewards & Recognitions</option>
                                                    <option value="Training & Career Devlopment">Training & Career Devlopment</option>
                                                    <option value="All of the Above">All of the Above</option>
                                                    <option value="Others">Others</option>
                                                </select>
                                                <label class="label-input100-new" for="name">
                                                    <span class="fas fa-tasks"></span>
                                                </label>
                                                <span class="focus-input100"></span>
                                            </div>

                                            <div class="wrap-input100" >
                                                <!--<span class="label-input100">Message</span>-->
                                                <textarea class="input100" name="message" placeholder="Your message here...(optional)"></textarea>
                                                <label class="label-input100-new" for="name">
                                                    <span class="fas fa-comment-alt"></span>
                                                </label>
                                                <span class="focus-input100"></span>
                                            </div>

                                            <div class="container-contact100-form-btn">
                                                <button class="contact100-form-btn" onclick="SendMailAttach()">
                                                    <span>
                                                        Submit
                                                        <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                                                    </span>
                                                </button>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
